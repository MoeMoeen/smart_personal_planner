# Environment Configuration for Smart Personal Planner
# Copy this to .env and uncomment the variables you want to set

# Agent Selection (default: complex)
# AGENT_TYPE=complex  # Use complex LangGraph agent (default)
# AGENT_TYPE=simple   # Use simple trust-based agent

# OpenAI Configuration
# OPENAI_API_KEY=your_openai_api_key_here

# Telegram Bot Configuration  
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Database Configuration
# DATABASE_URL=sqlite:///./smart_planner.db

# Logging Level
# LOG_LEVEL=INFO
